{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Generated schema for Root",
  "type": "object",
  "properties": {
    "snowflakes": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "label": {
            "type": "string"
          },
          "path": {
            "type": "string"
          },
          "position": {
            "type": "object",
            "description": "Position, described by web systems (px, %, rem, etc.)",
            "properties": {
              "x": {
                "type": "string"
              },
              "y": {
                "type": "string"
              }
            },
            "required": ["x", "y"]
          },
          "scale": {
            "type": "number"
          }
        },
        "required": ["label", "path", "position", "scale"]
      }
    }
  },
  "required": ["snowflakes"]
}
